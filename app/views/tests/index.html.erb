<h1>Test</h1>

<table>
    <tr>
      <th>Tests</th>
      <th>Questions count</th>
    </tr>
  <% @collection.each do |test| %>
    <tr>
        <td><%= test.title %></td>
        <% end %>
      <td>
        <% @collection.each do |test| %>
                <td><%= test.questions.count %></td>
          <% end %>
      </td>
    </tr>
</table>
